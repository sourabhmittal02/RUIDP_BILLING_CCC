                    $('#ComplaintTypeId').val(0);
                    $('h5').text('Complaint Type => ALL');
                }
                else if (this.value == 'SEWAR') {
                    $('#SEWAR').val(2);
                    $('h5').text('Complaint Type => SEWAR');
                }
                else if (this.value == 'WATER') {
                    $('#WATER').val(3);
                    $('h5').text('Complaint Type => WATER');
                }
                else if (this.value == 'ROAD') {
                    $('#ROAD').val(4);
                    $('h5').text('Complaint Type => ROAD');
                }
                else if (this.value == 'OTHER') {
