        function exportclick() {
            var Complainttype = 0;
            if ($('#All').prop('checked')) {
                Complainttype = 0;
            } else if ($('#SEWAR').prop('checked')) {
                Complainttype = 2;
            } else if ($('#WATER').prop('checked')) {
                Complainttype = 3;
            } else if ($('#ROAD').prop('checked')) {
                Complainttype = 4;
            } else if ($('#OTHER').prop('checked')) {
                Complainttype = 5;
            }

            $('#ComplaintTypeId').val(Complainttype);
            document.getElementById('Export').style.display = '';
            document.getElementById('btnExport').style.display = 'none';
        }
